{
	"$schema": "https://biomejs.dev/schemas/2.0.5/schema.json",
	"formatter": {
		"lineWidth": 200,
		"indentWidth": 8,
		"indentStyle": "tab"
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"trailingCommas": "none",
			"semicolons": "asNeeded"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"noExtraBooleanCast": "error"
			},
			"correctness": {
				"noUnusedImports": "error"
			}
		}
	}
}
